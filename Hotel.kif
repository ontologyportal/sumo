(=>
  (roomAmenity ?HR TemperatureControl)
  (exists (?DEVICE ?ROOM ?TEMP)
    (and
      (instance ?ROOM ?HR)
      (instance ?DEVICE AirConditioner)
      (instance ?TEMP TemperatureControl)
