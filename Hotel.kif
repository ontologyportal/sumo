      (part ?TEMP ?DEVICE))))


;; additional axiom for InRoom movies
(=>
  (roomAmenity ?ROOM MotionPicture)
  (exists (?R ?MS ?MP)
    (and
      (instance ?R ?ROOM)
      (instance ?MS MediaSystem)
      (located ?MS ?R)
      (instance ?MP MotionPicture)
      (modalAttribute
        (exists (?RL)
          (and
            (instance ?RL RadiatingLight)
            (instrument ?RL ?MS)
            (patient ?RL ?MP))) Possibility))))

