(subclass BreadOrBiscuit PreparedFood)
(typicalPart WheatFlour BreadOrBiscuit)
(documentation BreadOrBiscuit EnglishLanguage "Food that consists largely of &%WheatFlour
and water.  Note that this class covers crackers, cookies, as well as any
self_connected instance of bread, whether it is a loaf, a slice, a chunk
of bread, etc.")

(=>
  (instance ?X BreadOrBiscuit)
  (attribute ?X CookedAttribute))

(=>
  (instance ?B BreadOrBiscuit)
  (exists (?D ?BAKE)
    (and
      (instance ?D Dough)
      (instance ?BAKE Baking)
      (resource ?BAKE ?D)
      (result ?BAKE ?B))))
        
(subclass CookedDish PreparedFood)
(documentation CookedDish EnglishLanguage "A &%CookedDish is a specific food 
prepation, a 'distinct article or variety of food', ready to eat or to be served. [Wikipedia]")
(termFormat EnglishLanguage CookedDish "cooked dish")
      
(subclass Sandwich CookedDish)
(documentation Sandwich EnglishLanguage "Any food which consists of two or more pieces
of bread and some sort of filling between the two pieces of bread.")

(=>
  (instance ?S Sandwich)
  (exists (?B1 ?B2 ?F)
    (and
      (instance ?B1 BreadOrBiscuit)
      (instance ?B2 BreadOrBiscuit)
      (instance ?F (FoodForFn Human))
      (between ?B1 ?F ?B2)
      (not (equal ?B1 ?B2))
      (not (equal ?B1 ?F))
      (not (equal ?B2 ?F))
      (part ?B1 ?S)
      (part ?B2 ?S)
      (part ?F ?S))))

(subclass Dough PreparedFood)
(subclass Dough Mixture)
(documentation Dough EnglishLanguage "A &%Mixture of &%Flour, &%DrinkingWater, and possibly
other ingredients (such as &%Butter and &%Salt), which is used in
making &%BreadOrBiscuits or &%Pasta or noodles.")

(=>
  (instance ?D Dough)
  (attribute ?D RawAttribute))

(=>
  (instance ?D Dough)
  (exists (?F ?W)
    (and
      (instance ?F Flour)
      (instance ?W DrinkingWater)
      (part ?F ?D)
      (part ?W ?D))))

(subclass SoupStock PreparedFood)
(subclass SoupStock Soup)
(roomTempState SoupStock Liquid)
(documentation SoupStock EnglishLanguage "Food which is prepared by reducing &%Meat
and/or &%FruitOrVegetables to a translucent broth which can be used as
a base for soups or sauces.")
(names "Broth" SoupStock)

(=>
  (instance ?S SoupStock)
  (exists (?H)
    (and
      (instance ?H Heating)
      (result ?H ?S))))
      
(=>
  (and
    (subclass ?S Mixture) 
    (initialPart ?S Meat)
    (initialPart ?S FoodFromPlant)
    (initialPart ?S DrinkingWater))
  (equal ?S SoupStock))

(=>
  (instance ?S SoupStock)
  (attribute ?S Translucent))

(subclass Soup CookedDish)
(roomTempState Soup Liquid)
(disjoint Soup Sauce)
(disjoint Soup Beverage)
(documentation Soup EnglishLanguage "&%Soup is a &%CookDish that is not a &%Sauce nor a &%Beverage.")
(termFormat EnglishLanguage Soup "soup")
               
(instance ingredientAmount TernaryPredicate)
(documentation ingredientAmount EnglishLanguage "(&%ingredientAmount ?ING ?FOOD ?AMT 
means that ?AMT of ?ING is used to make ?FOOD")
(termFormat EnglishLanguage ingredientAmount "amount of ingredient")
(domainSubclass ingredientAmount 1 SelfConnectedObject)
(domainSubclass ingredientAmount 2 SelfConnectedObject)
(domain ingredientAmount 3 PhysicalQuantity)

(=>
  (ingredientAmount ?ING ?FOOD ?AMT)
  (exists (?PROC ?X ?Y)
    (and
      (instance ?X ?ING)
      (measure ?X ?AMT)
      (instance ?Y ?FOOD)
      (instance ?PROC Process)
      (patient ?PROC ?X)
      (result ?PROC ?Y))))

