        (instance ?B Butter)))))
        
(=>
  (and
    (instance ?C ChurningButter)
    (instance ?E Emulsion)
    (patient ?C ?E))
  (exists (?M ?B)
    (and
      (holdsDuring
        (BeginFn
          (WhenFn ?C))
        (and
          (equal ?E ?M)
          (instance ?M Milk)))
      (holdsDuring
        (EndFn
          (WhenFn ?C))
        (and
          (equal ?E ?B)
          (instance ?B Butter))))))
     
