(=>
  (instance ?B BrakePad)
  (hasPurpose ?B 
    (exists (?F ?BS)
      (and
        (instance ?F Friction)
        (resource ?F ?B)
        (instance ?BR BrakeRotor)
        (patient ?F ?BS)))))

(=>
  (and
    (instance ?BP BrakePad)
    (instance ?BR BrakeRotor)
    (instance ?R Rotating)
    (patient ?R ?BR)
    (instance ?F Friction)
    (resource ?F ?BP)
    (patient ?F ?BR))
  (not
    (patient ?R ?BP)))

(=>
  (instance ?B BrakeShoe)
  (hasPurpose ?B 
    (exists (?F ?BD)
      (and
        (instance ?F Friction)
        (resource ?F ?B)
        (instance ?BD BrakeDrum)
        (patient ?F ?BD)))))

(=>
  (and
    (instance ?BS BrakeShoe)
    (instance ?BD BrakeDrum)
    (instance ?R Rotating)
    (patient ?R ?BD)
    (instance ?F Friction)
    (resource ?F ?BS)
    (patient ?F ?BD))
  (not
    (patient ?R ?BS)))

(subclass DiscBrake VehicleBrake)

