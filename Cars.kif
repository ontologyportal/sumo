    (holdsDuring 
      (BeginFn ?P)
      (located ?A ?ALOC))
    (destination ?P ?D))
  (holdsDuring 
    (ImmediateFutureFn 
      (BeginFn ?P))
    (and
      (distance ?ALOC ?D ?SMALLER)
      (distance ?ALOC ?O ?LARGER)
      (greaterThan ?LARGER ?SMALLER))))

