(subclass FourWheelDriveVehicle LandVehicle)
(termFormat EnglishLanguage FourWheelDriveVehicle "four wheel drive")
(documentation FourWheelDriveVehicle EnglishLanguage "A &%LandVehicle
that is capable of providing power to all four &%Wheels.")

(=>
  (instance ?FWD FourWheelDriveVehicle)
  (hasPurpose ?FWD
    (forall (?W)
      (and
        (part ?W ?FWD)
        (exists (?E ?C ?R1 ?R2)
          (and
            (instance ?E Engine)
            (part ?E ?FWD)
            (instance ?C Crankshaft)
            (part ?C ?E)
