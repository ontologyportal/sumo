  (hasPurpose ?TWD
    (exists (?W1 ?W2)
      (and
        (part ?W1 ?TWD)
        (part ?W2 ?TWD)
        (not
          (equal ?W1 ?W2))
        (exists (?E ?C ?R1 ?R2 ?R3)
          (and
            (instance ?E Engine)
            (part ?E ?TWD)
            (instance ?C Crankshaft)
            (part ?C ?E)
            (instance ?R1 Rotating)
            (patient ?R1 ?C)
            (instance ?R2 Rotating)
            (patient ?R2 ?W1)
            (causes ?R1 ?R2)
            (patient ?R3 ?W2)
            (causes ?R1 ?R3)
            (not
              (exists (?W3 ?R4)
                (and
                  (not 
                    (equal ?W1 ?W3))
                  (not
                     (equal ?W2 ?W3))
                  (part ?W3 ?TWD)
                  (instance ?R4 Rotating)
                  (patient ?R4 ?W3)
                  (causes ?R1 ?R4))))))))))

