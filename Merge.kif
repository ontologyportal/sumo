(=>
  (and
    (instance ?D Decreasing)
    (patient ?D ?OBJ))
  (exists (?N1 ?N2 ?T1 ?T2 ?U)
    (and
      (earlier ?T1 ?T2)
      (holdsDuring ?T1
        (measure ?OBJ
          (MeasureFn ?N1 ?U)))
      (holdsDuring ?T2
        (measure ?OBJ
          (MeasureFn ?N2 ?U)))
      (lessThan ?N1 ?N2))))

(subclass Cooling Decreasing)

(documentation Cooling EnglishLanguage "Any &%Decreasing &%Process where the
&%PhysicalQuantity decreased is a &%TemperatureMeasure.")

(=>
   (and
      (instance ?COOL Cooling)
      (patient ?COOL ?OBJ))
