      (holdsDuring (EndFn (WhenFn ?HIRE)) (employs ?ORG ?PERSON))))

(subclass TerminatingEmployment LeavingAnOrganization)

(documentation TerminatingEmployment EnglishLanguage "&%OrganizationalProcesses where
