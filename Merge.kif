    (holdsDuring (BeginFn (WhenFn ?LEAVE)) (member ?P ?ORG))
    (holdsDuring (EndFn (WhenFn ?LEAVE)) (not (member ?P ?ORG)))))
      
(subclass Volunteering Offering)
(documentation Volunteering EnglishLanguage "&%Offering to work without
financial compensation.")

