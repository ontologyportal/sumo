;; Salt Water
(subclass SaltWater Water)
(documentation SaltWater EnglishLanguage "Naturally occurring salty water, especially seawater, or water to which salt has been added.")
(termFormat EnglishLanguage SaltWater "salt water")

(=>
  (instance ?SW SaltWater)
  (exists (?H2O ?SYN ?SALT)
    (and
      (instance ?H2O Water)
      (instance ?SALT SodiumChloride)
      (instance ?SYN ChemicalSynthesis)
      (resource ?SYN ?SALT)
      (resource ?SYN ?H2O)
      (result ?SYN ?SW))))


;; SaltWaterArea - UPDATE
;ADD below definition
(=>
  (instance ?AREA SaltWaterArea)
  (exists (?BED ?HOLE ?SW)
    (and
      (equal
        (HoleHostFn ?HOLE) ?BED)
      (instance ?SW SaltWater)
      (properlyFills ?SW ?HOLE)
      (equal
        (MereologicalSumFn ?BED ?SW) ?AREA))))


;; HookShape
(instance HookShape ShapeAttribute)
(documentation HookShape EnglishLanguage "A hook shape is a curved form resembling the letter J or a partially open loop, typically with one end straight and the other curving sharply inward.")
(termFormat EnglishLanguage HookShape "hook shape")

(=>
  (attribute ?OBJ HookShape)
  (attribute ?OBJ Concave))


;; Tailhook
(subclass Tailhook Device)
(documentation Tailhook EnglishLanguage "A tailhook, arresting hook, or arrester hook is a device attached to the empennage (rear) of some military fixed-wing aircraft. The hook is used to achieve rapid deceleration during routine landings aboard aircraft carrier flight decks at sea, or during emergency landings or aborted takeoffs at properly equipped airports. [from Wikipedia]")
(termFormat EnglishLanguage Tailhook "tailhook")

(=>
  (instance ?TH Tailhook)
  (shape ?TH HookShape))

(=>
  (instance ?TH Tailhook)
  (hasPurpose ?TH
    (exists (?PLANE ?STOP)
      (and
        (instance ?PLANE Airplane)
        (part ?TH ?PLANE)
        (instance ?STOP Stopping)
        (instrument ?STOP ?TH)
        (objectTransferred ?STOP ?PLANE)))))
