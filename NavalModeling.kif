;; Bow Ramp
(subclass BowRamp Ramp)
(documentation BowRamp EnglishLanguage "A bow ramp is a ramp on the bow (or front) of a boat or ship used for onload or offload of personnel or equipement.")
(termFormat EnglishLanguage BowRamp "bow ramp")

(=>
  (instance ?BR BowRamp)
  (exists (?WV)
    (and
      (instance ?WV WaterVehicle)
      (part ?BR ?WV)
      (equal ?BR
        (FrontFn ?WV)))))


;; Stern Ramp
(subclass SternRamp Ramp)
(documentation SternRamp EnglishLanguage "A stern ramp is a ramp on the stern (or back) of a boat or ship used for onload or offload of personnel or equipement.")
(termFormat EnglishLanguage SternRamp "stern ramp")

(=>
  (instance ?SR SternRamp)
  (exists (?WV)
    (and
      (instance ?WV WaterVehicle)
      (part ?SR ?WV)
      (equal ?SR
        (BackFn ?WV)))))


;; Military Building
(subclass MilitaryBuilding Building)
(documentation MilitaryBuilding EnglishLanguage "A Military Building is a structure used specifically for military purposes, such as command centers, barracks, storage facilities, and vehicle depots. Military Buildings are part of a MilitaryInstallation.")
(termFormat EnglishLanguage MilitaryBuilding "military building")

(=>
  (instance ?MB MilitaryBuilding)
  (exists (?MI)
    (and
      (instance ?MI MilitaryInstallation)
      (component ?MB ?MI))))


;; Vehicle Stowage Area
(subclass VehicleStowageArea Facility)
(documentation VehicleStowageArea EnglishLanguage "A Vehicle Stowage Area is a designated building or compartment in a military installation or ship where military vehicles are stored, secured, and staged for deployment.")
(termFormat EnglishLanguage VehicleStowageArea "vehicle stowage area")

(=>
  (instance ?VSA VehicleStowageArea)
  (hasPurpose ?VSA
    (exists (?MV)
      (and
        (instance ?MV MilitaryVehicle)
        (orientation ?MV ?VSA Inside)))))

(=>
  (instance ?VSA VehicleStowageArea)
  (and
    (or
      (instance ?VSA MilitaryBuilding)
      (instance ?VSA ShipCompartment))
    (not
      (and
        (instance ?VSA MilitaryBuilding)
        (instance ?VSA ShipCompartment)))))


