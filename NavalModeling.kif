;; JetAirplane - UPDATE
(subclass JetAirplane Airplane)
(documentation JetAirplane EnglishLanguage "A jet aircraft (or simply jet) is an aircraft (nearly always a fixed-wing aircraft) propelled by one or more jet engines. [from Wikipedia]")
(termFormat EnglishLanguage JetAirplane "jet")

(=>
  (instance ?JET JetAirplane)
  (exists (?T)
    (and
      (instance ?T Turbine)
      (part ?T ?JET))))


;; Turbine - UPDATE
(=>
  (instance ?T Turbine)
  (hasPurpose ?T
    (exists (?FLUIDIN ?FLUIDOUT ?N ?N2)
      (and
        (instance ?FLUIDIN Fluid)
        (instance ?FLUIDOUT Fluid)
        (contains ?T ?FLUIDIN)
        (not
          (contains ?T ?FLUIDOUT))
        (measure ?FLUIDIN
          (MeasureFn ?N Pascal))
        (measure ?FLUIDOUT
          (MeasureFn ?N2 Pascal))
        (greaterThan ?N ?N2)))))

(=>
  (instance ?T Turbine)
  (hasPurpose ?T
    (exists (?PG)
      (and
        (instance ?PG PowerGeneration)
        (instrument ?PG ?T)))))


;; Arresting Gear
(subclass ArrestingGear Device)
(documentation ArrestingGear EnglishLanguage "An arresting gear, or arrestor gear, is a mechanical system used to rapidly decelerate an aircraft as it lands. [from Wikipedia]")
(termFormat EnglishLanguage ArrestingGear "arresting gear")

(=>
  (instance ?AG ArrestingGear)
  (hasPurpose ?AG
    (exists (?PLANE ?TH ?STOP ?CONTACT)
      (and
        (instance ?PLANE Airplane)
        (instance ?TH Tailhook)
        (instance ?STOP Stopping)
        (instance ?CONTACT Attaching)
        (instrument ?CONTACT ?TH)
        (patient ?CONTACT ?PLANE)
        (patient ?CONTACT ?AG)
        (causes ?CONTACT ?STOP)
        (objectTransferred ?STOP ?PLANE)))))


;; Catapulting
(subclass Catapulting Impelling)
(documentation Catapulting EnglishLanguage "The subclass of Impelling where the instrument of Catapulting is a Catapult, and the objectTransferred is an Object. The Object starts in contact with the Catapult and ends in the air.")
(termFormat EnglishLanguage Catapulting "catapulting")

(=>
  (instance ?CATG Catapulting)
  (exists (?OBJ ?AIR ?CATAPULT)
    (and
      (instance ?OBJ Object)
      (instance ?AIR Atmosphere)
      (instance ?CATAPULT Catapult)
      (objectTransferred ?CATG ?OBJ)
      (instrument ?CATG ?CATAPULT)
      (holdsDuring
        (BeginFn
          (WhenFn ?CATG))
        (meetsSpatially ?OBJ ?CATAPULT))
      (holdsDuring
        (EndFn
          (WhenFn ?CATG))
        (located ?OBJ ?AIR)))))


;; Catapult
(subclass Catapult Device)
(documentation Catapult EnglishLanguage "A catapult is a ballistic device used to launch a projectile a great distance without the aid of gunpowder or other propellants. [from Wikipedia]")
(termFormat EnglishLanguage Catapult "catapult")

(=>
  (instance ?CAT Catapult)
  (hasPurpose ?CAT
    (exists (?OBJ ?CATG)
      (and
        (instance ?OBJ Object)
        (instance ?CATG Catapulting)
        (instrument ?CATG ?CAT)
        (objectTransferred ?CATG ?OBJ)))))


;; Aircraft Catapult
(subclass AircraftCatapult Catapult)
(documentation AircraftCatapult EnglishLanguage "An aircraft catapult is a device used to help fixed-wing aircraft gain enough airspeed and lift for takeoff from a limited distance, typically from the deck of a ship. [from Wikipedia]")
(termFormat EnglishLanguage AircraftCatapult "aircraft catapult")

(=>
  (instance ?ACAT AircraftCatapult)
  (hasPurpose ?ACAT
    (exists (?PLANE ?CATG)
      (and
        (instance ?PLANE Airplane)
        (instance ?CATG Catapulting)
        (instrument ?CATG ?ACAT)
        (objectTransferred ?CATG ?PLANE)))))

(=>
  (instance ?ACAT AircraftCatapult)
  (exists (?CARRIER)
    (and
      (instance ?CARRIER AircraftCarrier)
      (part ?ACAT ?CARRIER))))


;; AirSearchRadar
(subclass AirSearchRadar Radar)
(documentation AirSearchRadar EnglishLanguage "A radar that sweeps the air for contacts.")
(termFormat EnglishLanguage AirSearchRadar "air search radar")

(=>
  (instance ?ASR AirSearchRadar)
  (hasPurpose ?ASR
    (exists (?PLANE ?DISCOVER)
      (and
        (instance ?PLANE Airplane)
        (instance ?DISCOVER Discovering)
        (instrument ?DISCOVER ?ASR)
        (patient ?DISCOVER ?PLANE)))))


;; Surface Search Radar
(subclass SurfaceSearchRadar Radar)
(documentation SurfaceSearchRadar EnglishLanguage "A radar that sweeps the surface for contacts.")
(termFormat EnglishLanguage SurfaceSearchRadar "surface search radar")

(=>
  (instance ?SSR SurfaceSearchRadar)
  (hasPurpose ?SSR
    (exists (?BOAT ?DISCOVER)
      (and
        (instance ?BOAT WaterVehicle)
        (instance ?DISCOVER Discovering)
        (instrument ?DISCOVER ?SSR)
        (patient ?DISCOVER ?BOAT)))))


;; Navigation Radar
(subclass NavigationRadar Radar)
(documentation NavigationRadar EnglishLanguage "A navigation radar is a radar system used to detect and track objects, obstacles, and coastlines, providing real-time situational awareness to assist in the safe movement and positioning of a vessel, aircraft, or vehicle.")
(termFormat EnglishLanguage NavigationRadar "navigation radar")

(=>
  (instance ?NAVR NavigationRadar)
  (hasPurpose ?NAVR
    (exists (?VEH ?NAV)
      (and
        (instance ?VEH Vehicle)
        (instance ?NAV Navigation)
        (instrument ?NAV ?NAVR)
        (patient ?NAV ?VEH)))))


;; Navigation
(subclass Navigation IntentionalProcess)
(documentation Navigation EnglishLanguage "Navigation is the process of determining position, planning a route, and moving through an environment—whether by a person, animal, vehicle, or system—to reach a desired destination.")
(termFormat EnglishLanguage Navigation "navigation")

(=>
  (instance ?NAV Navigation)
  (exists (?NP ?TRANS)
    (and
      (instance ?NP NavigationPlanning)
      (instance ?TRANS Translocation)
      (subProcess ?NP ?NAV)
      (subProcess ?TRANS ?NAV))))

;; Navigation Plan
(subclass NavigationPlan Plan)
(documentation NavigationPlan EnglishLanguage "A navigation plan is a specification of a sequence of processes that is intended to ensure the successful movement of a person, animal, vehicle, or system from a starting position to a specified destination at some future time.")
(termFormat EnglishLanguage NavigationPlan "navigation plan")

(=>
  (instance ?NP NavigationPlan)
  (hasPurpose ?NP
    (exists (?TRANS)
      (and
        (instance ?TRANS Translocation)
        (represents ?NP ?TRANS)))))


;; Navigation Planning
(subclass NavigationPlanning Planning)
(documentation NavigationPlanning EnglishLanguage "Specifying a set of actions in order to create a NavigationPlan necessary to navigate from an origin to a destination.")
(termFormat EnglishLanguage NavigationPlanning "navigation planning")

(=>
  (instance ?NAVPLANNING NavigationPlanning)
  (hasPurpose ?NAVPLANNING
    (exists (?NP)
      (and
        (instance ?NP NavigationPlan)
        (result ?NAVPLANNING ?NP)))))

