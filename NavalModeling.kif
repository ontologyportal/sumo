(=>
  (instance ?LHA LandingHelicopterAssaultShip)
  (exists (?CT)
    (and
      (instance ?CT ControlTower)
      (component ?CT ?LHA))))

(=>
  (instance ?LHA LandingHelicopterAssaultShip)
  (exists (?FD)
    (and
      (instance ?FD FlightDeck)
      (part ?FD ?LHA))))

(=>
  (instance ?LHA LandingHelicopterAssaultShip)
  (exists (?VSA)
