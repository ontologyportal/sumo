;; Landing Craft ---------------------------------------------------------------
(subclass LandingCraft AmphibiousWarfareShip)
(subclass LandingCraft PassengerShip)
(documentation LandingCraft EnglishLanguage "Landing craft are rugged, steel displacement vessels used by amphibious forces to transport equipment and troops from ship to the shore, along the shore, and from shore back to the amphibious warships. Landing craft are also used to support civilian humanitarian/ maritime operations. Landing craft are capable of transporting cargo, tracked and/ or wheeled vehicles and troops from amphibious assault ships to suitable beachheads or piers [from www.navy.mil].")
(termFormat EnglishLanguage LandingCraft "landing craft")

(=>
  (instance ?LC LandingCraft)
  (hasPurpose ?LC
    (exists (?STUFF ?SHORE ?WATER ?DELIVER ?PEOPLE ?MILVEH ?EQUIP)
      (and
        (instance ?SHORE ShoreArea)
        (instance ?WATER WaterArea)
        (instance ?DELIVER WaterTransportationService)
        (patient ?DELIVER ?STUFF)
        (origin ?DELIVER ?WATER)
        (destination ?DELIVER ?SHORE)
        (instance ?PEOPLE Human)
        (instance ?MILVEH MilitaryVehicle)
        (instance ?EQUIP Device)
        (or
          (equal ?PEOPLE ?STUFF)
          (equal ?MILVEH ?STUFF)
          (equal ?EQUIP ?STUFF))))))

;; ESD - ALREADY DONE BY ANOTHER STUDENT
(subclass ExpeditionaryTransferDockShip MilitaryShip)
(documentation ExpeditionaryTransferDockShip EnglishLanguage "An Expeditionary Transfer Dock (ESD), formerly the Mobile Landing Platform (MLP), is designed to be a semi-submersible, flexible, modular platform providing the US Navy with the capability to perform large-scale logistics movements such as the transfer of vehicles and equipment from sea to shore. These ships significantly reduce the dependency on foreign ports and provide support in the absence of port availability. The class also houses a sub-class variant called the Expeditionary Mobile Base (ESB), formerly the Afloat Forward Staging Base (AFSB). [from Wikipedia]")
(termFormat EnglishLanguage ExpeditionaryTransferDockShip "ESD")

(=>
  (instance ?ESD ExpeditionaryTransferDockShip)
  (hasPurpose ?ESD
    (exists (?STUFF ?UNIT ?LOG ?WATER)
      (and
        (instance ?STUFF Object)
        (instance ?UNIT MilitaryUnit)
        (instance ?LOG Transfer)
        (instance ?WATER WaterArea)
        (agent ?LOG ?ESD)
        (objectTransferred ?LOG ?STUFF)
        (origin ?LOG ?WATER)
        (destination ?LOG ?UNIT)))))


;; Military Logistics
(subclass MilitaryLogistics MilitaryProcess)

(documentation MilitaryLogistics EnglishLanguage "Military Logistics is a specialized process that involves both the physical transfer of military personnel, equipment, and supplies, as well as the strategic planning required to ensure operational readiness. It encompasses transportation, supply chain management, maintenance, and infrastructure coordination.")
(termFormat EnglishLanguage MilitaryLogistics "military logistics")

(=>
  (instance ?ML MilitaryLogistics)
  (exists (?LP ?LT)
    (and
      (instance ?LP LogisticsPlanning)
      (instance ?LT LogisticsTransfer)
      (subProcess ?LP ?ML)
      (subProcess ?LT ?ML))))


;; Logistics Planning
(subclass LogisticsPlanning Planning)
(documentation Planning EnglishLanguage "Specifying a set of actions in order to meet a set of goals or objectives in support of a MilitaryLogistics operation.")
(termFormat EnglishLanguage LogisticsPlanning "logistics planning")

(=>
  (instance ?PLANNING LogisticsPlanning)
  (hasPurpose ?PLANNING
    (exists (?LP)
      (and
        (instance ?LP LogisticsPlan)
        (result ?PLANNING ?LP)))))

;; Logistics Transfer
(subclass LogisticsTransfer Transfer)
(documentation Transfer EnglishLanguage "Any instance of Transfer that supports MilitaryLogistics operations.")
(termFormat EnglishLanguage LogisticsTransfer "logistics transfer")

(=>
  (instance ?LT LogisticsTransfer)
  (hasPurpose ?LT
    (exists (?STUFF ?UNIT)
      (and
        (instance ?UNIT MilitaryUnit)
        (objectTransferred ?LT ?STUFF)
        (holdsDuring
          (EndFn
            (WhenFn ?LT))
          (possesses ?UNIT ?STUFF))))))

;; Logistics Plan
(subclass LogisticsPlan Plan)

(documentation LogisticsPlan EnglishLanguage "A plan that outlines logistics support for military or organizational operations, including supply, transportation, maintenance, and sustainment to ensure operational readiness.")
(termFormat EnglishLanguage LogisticsPlan "logistics plan")

(=>
  (instance ?LP LogisticsPlan)
  (hasPurpose ?LP
    (exists (?LT)
      (and
        (instance ?LT LogisticsTransfer)
        (represents ?LP ?LT)))))


;; Plan - UPDATE
(subclass Plan Procedure)
(documentation Plan EnglishLanguage "A specification of a sequence of Processes which is intended to satisfy a specified purpose at some future time.")
(termFormat EnglishLanguage Plan "plan")

(=>
  (and
    (instance ?PLAN Plan)
    (instance ?CBP ContentBearingPhysical)
    (containsInformation ?CBP ?PLAN))
  (exists (?PLANNING)
    (and
      (instance ?PLANNING Planning)
      (result ?PLANNING ?CBP))))


;; Filling
(subclass Filling Putting)
(documentation Filling EnglishLanguage "The Class of Processes where and number of things or amount of substances are put into an empty space and partiallyFills or completelyFills the empty space.")
(termFormat EnglishLanguage Filling "filling")

(=>
  (instance ?FILL Filling)
  (exists (?HOLE ?OBJ)
    (and
      (instance ?HOLE HoleRegion)
      (instance ?OBJ Object)
      (patient ?FILL HoleRegion)
      (objectTransferred ?FILL ?OBJ)
      (partiallyFills ?OBJ ?HOLE))))

;; Engine - UPDATE
(subclass Engine Transducer)
(documentation Engine EnglishLanguage "Engine is a subclass of Transducer. Engines are devices for converting some form of energy resource into mechanical power.")
(termFormat EnglishLanguage Engine "engine")

(=>
  (instance ?E Engine)
  (hasPurpose ?E
    (exists (?VEH ?MOVE)
      (and
        (instance ?VEH Vehicle)
        (instance ?MOVE Translocation)
        (instrument ?MOVE ?E)
        (patient ?MOVE ?VEH)))))

;; Steam Engine - UPDATE
(subclass SteamEngine Engine)
(documentation SteamEngine EnglishLanguage "SteamEngine is the subclass of Engines that produce mechanical power from heat and steam pressure.")
(termFormat EnglishLanguage SteamEngine "steam engine")

(=>
  (instance ?OP
    (OperatingFn SteamEngine))
  (exists (?S ?W ?B ?F)
    (and
      (instance ?W Water)
      (instance ?B Boiling)
      (instance ?S Steam)
      (instance ?F Fuel)
      (resource ?B ?F)
      (patient ?B ?W)
      (result ?B ?S)
      (causes ?B ?OP))))


;; LCU
(subclass LandingCraftUtility LandingCraft)
(documentation LandingCraftUtility EnglishLanguage "Mechanized and utility landing craft are rugged, steel displacement vessels used by amphibious forces to transport equipment and troops from ship to the shore, along the shore, and from shore back to the amphibious warships. LCUs have both bow and stern ramps for onload/ offload. [from navy.mil]")
(termFormat EnglishLanguage LandingCraftUtility "LCU")

(=>
  (instance ?LCU LandingCraftUtility)
  (exists (?BOWRAMP ?STERNRAMP)
    (and
      (instance ?BOWRAMP BowRamp)
      (instance ?STERNRAMP SternRamp)
      (part ?BOWRAMP ?LCU)
      (part ?STERNRAMP ?LCU))))


;; LCM
(subclass LandingCraftMechanized LandingCraft)
(documentation LandingCraftMechanized EnglishLanguage "Mechanized and utility landing craft are rugged, steel displacement vessels used by amphibious forces to transport equipment and troops from ship to the shore, along the shore, and from shore back to the amphibious warships. LCMs have a bow ramp for onload/ offload. [from navy.mil]")
(termFormat EnglishLanguage LandingCraftMechanized "LCM")

(=>
  (instance ?LCU LandingCraftUtility)
  (exists (?BOWRAMP)
    (and
      (instance ?BOWRAMP BowRamp)
      (part ?BOWRAMP ?LCU))))


;; LCAC 
(subclass LandingCraftAirCushion LandingCraft)
(subclass LandingCraftAirCushion Hovercraft)
(documentation LandingCraftAirCushion EnglishLanguage "Air cushion vehicle for transporting, ship-to-shore and across the beach, personnel, weapons, equipment, and cargo of the assault elements of the Marine Air-Ground Task Force. [from navy.mil]")
(termFormat EnglishLanguage LandingCraftAirCushion "LCAC")

(=>
  (instance ?LCAC LandingCraftAirCushion)
  (hasPurpose ?LCAC
    (exists (?MAGTF ?LT)
      (and
        (instance ?MAGTF MarineAirGroundTaskForce)
        (instance ?LT LogisticsTransfer)
        (agent ?LT ?MAGTF)
        (instrument ?LT ?LCAC)))))

;; Hovercraft
(subclass Hovercraft WaterVehicle)
(documentation Hovercraft EnglishLanguage "A hovercraft, also known as an air-cushion vehicle or ACV,[2] is an amphibious craft capable of travelling over land, water, mud, ice, and various other surfaces. [from Wikipedia]")
(termFormat EnglishLanguage Hovercraft "hovercraft")

(=>
  (instance ?HC Hovercraft)
  (hasPurpose ?HC
    (exists (?H ?WATERREGION ?LANDREGION ?FAN ?WIND)
      (and
        (instance ?H Hovering)
        (instance ?FAN FanDevice)
        (or
          (and
            (instance ?WATERREGION WaterArea)
            (orientation ?HC ?WATERREGION Above))
          (and
            (instance ?LANDREGION LandArea)
            (orientation ?HC ?LANDREGION Above)))
        (part ?FAN ?HC)
        (instance ?WIND Wind)
        (instrument ?H ?HC)
        (instrument ?WIND ?FAN)
        (causes ?WIND ?H)))))


; Hovering
(subclass Hovering Process)
(documentation Hovering EnglishLanguage "Hovering is a Process where an AGENT is suspended above a surface.")
(termFormat EnglishLanguage Hovering "hovering")

(=>
  (instance ?H Hovering)
  (exists (?OBJ ?SURF)
    (and
      (instance ?OBJ Object)
      (instance ?SURF Region)
      (or
        (experiencer ?H ?OBJ)
        (agent ?H ?OBJ)
        (instrument ?H ?OBJ))
      (orientation ?OBJ ?SURF Above))))



;; Flight Deck
(subclass FlightDeck Runway)
(documentation FlightDeck EnglishLanguage "the surface from which its aircraft take off and land, essentially a miniature airfield at sea. On smaller naval ships which do not have aviation as a primary mission, the landing area for helicopters and other VTOL aircraft is also referred to as the flight deck. [from Wikipedia]")
(termFormat EnglishLanguage FlightDeck "flight deck")

(=>
  (instance ?FD FlightDeck)
  (exists (?MS)
    (and
      (instance ?MS MilitaryShip)
      (part ?FD ?MS))))



;; Runway - UPDATE
;ADD the below definition
(=>
  (instance ?RW Runway)
  (hasPurpose ?RW
    (exists (?A ?T)
      (and
        (instance ?A Aircraft)
        (patient ?T ?A)
        (or
          (and
            (origin ?T ?RW)
            (instance ?T TakingOff))
          (and
            (destination ?T ?RW)
            (instance ?T Landing)))))))


