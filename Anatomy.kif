(=>
  (and
    (instance ?R ThoracicVertebraBone)
    (part ?R ?H)
    (instance ?H Human)
    (not 
      (attribute ?H DiseaseOrSyndrome)))
  (exists (?S)
    (and
      (instance ?S ThoracicVertebraBone)
      (not
        (equal ?S ?R))
      (part ?S ?H)
      (connected ?S ?R))))
      

