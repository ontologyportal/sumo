(=>
  (and
    (instance ?R TrueRibBone)
    (part ?R ?H)
    (instance ?H Human)
    (not 
      (attribute ?H DiseaseOrSyndrome)))
  (exists (?S)
    (and
      (instance ?S Sternum)
      (part ?S ?H)
      (connected ?S ?R))))
      
(=>
  (and
    (instance ?R FalseRibBone)
    (part ?R ?H)
    (instance ?H Human)
    (not 
      (attribute ?H DiseaseOrSyndrome)))
  (not 
    (exists (?S)
      (and
        (instance ?S Sternum)
        (part ?S ?H)
        (connected ?S ?R)))))
            
