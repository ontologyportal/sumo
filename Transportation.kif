  ?LENGTH))

(=>
  (and
    (lengthOfCrudeOilPipeline ?AREA ?AMOUNT1)
    (totalPipelineInArea ?AREA ?AMOUNT2))
  (lessThanOrEqualTo ?AMOUNT1 ?AMOUNT2))

