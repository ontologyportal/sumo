(subclass ForestProduct Product)
(documentation ForestProduct EnglishLanguage "&%Products from the &%Forest.")

(=>
  (and
    (instance ?P ForestProduct)
    (instance ?M Manufacture)
    (result ?M ?P)
    (patient ?M ?X)
    (instance ?X Organism))
  (exists (?A ?F)
    (and
      (instance ?A Agriculture)
      (result ?A ?X)
      (eventLocated ?A ?F)
      (instance ?F Forest))))

(subclass WoodProduct ForestProduct)
(subclass WoodProduct OrganicCompound)
(subclass WoodProduct PlantSubstance)
(documentation WoodProduct EnglishLanguage "&%Products made from &%Wood material.")

(=>
  (and
    (instance ?P WoodProduct)
    (part ?W ?P))
  (instance ?W Wood))

(=>
  (and
    (equal ?W
      (OrganicObjectFn ?S))
    (subclass ?W WoodProduct)
    (subclass ?S PlantStem))
  (exists (?T)
    (and
      (subclass ?T BotanicalTree)
      (initialPart ?S ?T))))

;; Changed OrganicObjectFn to OrganicObjectFn JP 20230517  
(instance OrganicObjectFn UnaryFunction)
(relatedInternalConcept OrganicObjectFn DeadFn)
(domainSubclass OrganicObjectFn 1 OrganicObject)
(rangeSubclass OrganicObjectFn Object)
(format EnglishLanguage OrganicObjectFn "&%Object made from %1")
(termFormat EnglishLanguage OrganicObjectFn "organic object fn")
(documentation OrganicObjectFn EnglishLanguage "(OrganicObjectFn ?ORGCLASS) denotes the &%Class of 
&%Object derived from an &%OrganicObject. The &%Object was once a living &%Organism or a 
living &%part of an &%Organism. For example, &%Hardwood is made from harvesting the 
&%PlantStems of a &%DeciduousTree. &%Juice is made from &%FruitOrVegetable of a &%Plant.")

(=>
  (and
    (equal ?P
      (OrganicObjectFn ?ORG))
    (subclass ?P Object))
  (or    
    (subclass ?ORG Organism)   
    (subclass ?ORG AnatomicalStructure)))
  
(=>
  (and
    (equal ?P
      (OrganicObjectFn ?ORG))
    (subclass ?P Object))
  (exists (?R ?O)
    (and
      (instance ?R (DeadFn ?ORG))
      (part ?R ?O)
      (instance ?O ?ORG))))
 
