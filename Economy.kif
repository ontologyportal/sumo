(termFormat EnglishLanguage LandMeanOfProduction "natural resource")

(=>
  (and
    (instance ?LE LandMeanOfProduction)
    (attribute ?LEO ?LE))
  (exists (?L ?EMS)
    (or
      (and
        (instance ?L Region)
        (part ?LEO ?L))
      (and
        (instance ?EMS ElectromagenticSpectrum)
        (equal ?LEO ?EMS)))))

