(subclass Felling Cutting)
(subclass Felling Harvesting)
(documentation Felling EnglishLanguage "The intentional &%Felling is the &%Cutting down of a &%BotanicalTree.")

(=>
  (instance ?FELL Felling)
  (exists (?C ?F ?T ?S)
    (and
      (instance ?C Cutting)
      (patient ?C ?T)
      (instance ?T BotanicalTree)
      (result ?C ?F)
      (instance ?F Falling)
      (patient ?F ?S)
      (instance ?S PlantStem)
      (part ?S ?T))))

(=>
  (and
    (instance ?F Felling)
    (result ?F ?S))
  (exists (?STEM)
    (and
      (instance ?S
        (DeadFn ?STEM))
      (subclass ?STEM PlantStem))))
          
(subclass Timber WoodProduct)
(documentation Timber EnglishLanguage "&%Timber are &%PlantStems of felled &%BotanicalTrees before they are 
cut and processed into &%Lumber.")
(relatedInternalConcept Timber OrganicObject)
(relatedInternalConcept Timber BotanicalTree)
(relatedInternalConcept Timber Lumber)

(=>
  (instance ?F Felling)
  (hasPurpose ?F
    (exists (?T ?TIMB ?S)
      (and
        (instance ?T BotanicalTree)
        (patient ?F ?T)
        (result ?F ?TIMB)
        (instance ?TIMB Timber)
        (equal ?TIMB ?S)
        (instance ?S PlantStem)
        (holdsDuring
          (BeginFn 
            (WhenFn ?F))
          (part ?S ?T))
        (holdsDuring
          (EndFn
            (WhenFn ?F))
          (not
            (part ?S ?T)))))))
        
(=>
  (instance ?L Lumber)
  (exists (?C ?T)
      (and
        (instance ?C Cutting)
        (result ?C ?L)
        (patient ?C ?T)
        (instance ?T Lumber))))
      
(subclass Lumber WoodProduct)
(documentation Lumber EnglishLanguage "&%Lumber is a class of &%WoodProduct cut into various sizes and forms
from &%Timber.")

(=>
  (instance ?L Lumber)
  (hasPurpose ?L
    (exists (?C)
      (and
        (instance ?C Carpentry)
        (patient ?C ?L)))))

(subclass RoughSawnLumber Lumber)
(documentation RoughSawnLumber EnglishLanguage "&%RoughSawnLumber is &%Lumber cut into a broad of desired
thickness, width and one straigth_line edge.")

(=>
  (instance ?R RoughSawnLumber)
  (not
    (exists (?C ?P)
      (and
        (instance ?C Cutting)
        (instrument ?C ?P)
        (instance ?P Planer)
        (patient ?C ?R)))))

(subclass FinishedLumber Lumber)
(documentation FinishedLumber EnglishLanguage "&%FinishedLumber is &%RoughSawnLumber that has been passed 
through a &%Planner.")

(=>
  (instance ?F FinishedLumber)
  (exists (?C ?P)
    (and
      (instance ?C Cutting)
      (instrument ?C ?P)
      (instance ?P Planer)
      (result ?C ?F))))

