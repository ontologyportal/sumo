;; JP 8Apr2023
;; Changed Spice to PlantSubstance

(subclass Spice PreparedFood)
(subclass Spice FoodFromPlant)
(documentation Spice EnglishLanguage "A &%Spice is a class of &%PreparedFood derived
from &%Seeds, &%Fruits, &%PlantRoots, barks, or other &%PlantAnatomicalStructure 
primarily used for flavoring or coloring food. ")

(=>
  (instance  ?S Spice)
  (exists (?P ?CLASS)
    (and
      (instance ?P
        (DeadFn ?CLASS))
      (part ?S ?P)
      (subclass ?CLASS PlantAnatomicalStructure))))

(=>
  (instance ?S Spice)  
  (hasPurpose ?S
    (exists (?PUT ?F)
      (and
        (instance ?PUT Putting)
        (patient ?PUT ?S)
        (destination ?PUT ?F)
        (instance ?F PreparedFood)))))

(=>
  (and
    (instance ?P Putting)
    (patient ?P ?S)
    (instance ?S Spice)
    (destination ?P ?F)
    (instance ?F PreparedFood))
  (exists (?C ?A)
    (and
      (instance ?C InternalChange)
      (patient ?C ?F)
      (result ?C ?A)
      (attribute ?F ?A)
      (or
        (instance ?A FlavourAttribute)
        (instance ?A ColorAttribute)))))
                    
