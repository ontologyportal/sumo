(=>
  (instance ?EMPLOYER Employer)
  (exists (?HIRING ?EMPLOYEE)
    (and
      (instance ?HIRING Hiring)
      (instance ?EMPLOYEE Employee)
      (agent ?HIRING ?EMPLOYER)
      (patient ?HIRING ?EMPLOYEE))))

(documentation Employee EnglishLanguage "A worker who is hired to perform a job.")
(instance Employee SocialRole)
(termFormat EnglishLanguage Employee "employee")

(=>
  (instance ?EMPLOYEE Employee)
  (exists (?HIRING)
    (and
      (instance ?HIRING Hiring)
      (instance ?EMPLOYEE Employee)
      (agent ?HIRING ?EMPLOYER)
      (patient ?HIRING ?EMPLOYEE))))

(<=>
  (employs ?EMPLOYER ?EMPLOYEE)
  (and
    (instance ?EMPLOYER Employer)
    (instance ?EMPLOYEE Employee)))



