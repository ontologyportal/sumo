for ?AREA.")

(=>
  (and
    (holdsDuring ?TIME (capitalExpendituresOfArea ?AREA ?CAPAMOUNT))
    (holdsDuring ?TIME (annualExpendituresOfArea ?AREA ?TOTALAMOUNT)))
  (greaterThan ?TOTALAMOUNT ?CAPAMOUNT))


