(termFormat EnglishLanguage GeochronologicSuperEon "supereon")
(termFormat PortugueseLanguage GeochronologicSuperEon "super éon")

(subclass GeochronologicEon GeochronologicTime)
(termFormat EnglishLanguage GeochronologicEon "eon")
(termFormat PortugueseLanguage GeochronologicEon "éon")

(subclass GeochronologicEra GeochronologicTime)
(termFormat EnglishLanguage GeochronologicEra "era")
(termFormat PortugueseLanguage GeochronologicEra "era")

(subclass GeochronologicPeriod GeochronologicTime)
(termFormat EnglishLanguage GeochronologicPeriod "period")
(termFormat PortugueseLanguage GeochronologicPeriod "período")

(subclass GeochronologicSubPeriod GeochronologicTime)
(termFormat EnglishLanguage GeochronologicSubPeriod "sub period")

(subclass GeochronologicEpoch GeochronologicTime)
(termFormat EnglishLanguage GeochronologicEpoch "epoch")
(termFormat PortugueseLanguage GeochronologicEpoch "época")

(subclass GeochronologicAge GeochronologicTime)
(termFormat EnglishLanguage GeochronologicAge "age")
(termFormat PortugueseLanguage GeochronologicAge "idade")


;; there are no two GeochronologicTime in the same rank overlapping

(=> (and (instance ?X GeochronologicTime)
       (instance ?Y GeochronologicTime)
       (instance ?X ?Class)
       (instance ?Y ?Class)
       (not (equal ?X ?Y))
       (subclass ?class GeochronologicTime)
  (not (overlapsTemporally ?X ?Y)))


;; The hierarchy is: SuperEon > Eon > Era > Period > SubPeriod (where
;; one exists) > Epoch > Age

(=> (instance ?X GeochronologicAge) 
    (exists (?Y)
       (and (instance ?Y GeochronologicEpoch) 
	    (or (starts ?X ?Y)  
		(during ?X ?Y)
		(finishes ?X ?Y)))))

(=> (instance ?X GeochronologicEpoch) 
    (exists (?Y)
       (and
          (or (instance ?Y GeochronologicPeriod) 
              (instance ?Y GeochronologicSubPeriod))
	  (or (starts ?X ?Y)  
	      (during ?X ?Y)
	      (finishes ?X ?Y)))))

(=> (instance ?X GeochronologicSubPeriod) 
    (exists (?Y)
       (and (instance ?Y GeochronologicPeriod) 
	    (or (starts ?X ?Y)  
		(during ?X ?Y)
		(finishes ?X ?Y)))))
		
(=> (instance ?X GeochronologicPeriod) 
    (exists (?Y)
       (and (instance ?Y GeochronologicEra) 
	    (or (starts ?X ?Y)  
		(during ?X ?Y)
		(finishes ?X ?Y)))))

(=> (instance ?X GeochronologicEra) 
    (exists (?Y)
       (and (instance ?Y GeochronologicEon) 
	    (or (starts ?X ?Y)  
		(during ?X ?Y)
		(finishes ?X ?Y)))))


;; the boundaries

(subclass GeochronologicBase TimePoint)

(instance GeochronologicPresent (BeginFn (YearFn 1950))

(instance MillionYearsAgoFromPresent UnaryFunction)
(domain MillionYearsAgoFromPresent 1 Number)
(range MillionYearsAgoFromPresent 1 TimePoint)

(equal 
 (MillionYearsAgoFn ?X) 
 (BeginFn (YearFn (AdditionFn 1950 (MultiplicationFn ?X -1000000)))))

(instance Ma+- TernaryPredicate)
(domain Ma+- 1 GeochronologicBase)
(domain Ma+- 2 Number)
(domain Ma+- 2 Number)

(=> (Ma+- ?Base ?X ?Y)
    (exists (?Y)
       (and (after ?Base (MillionYearsAgoFn (AdditionFn ?X ?Y)))) 
	    (before ?Base (MillionYearsAgoFn (SubtractionFn ?X ?Y)))))

(instance Ma~ BinaryPredicate)
(domain Ma+- 1 GeochronologicBase)
(domain Ma+- 2 Number)

(=> (Ma~ ?Base ?X)
    (exists (?Y)
       (and (approximateValue ?X ?Y)
	    (equal ?Base (MillionYearsAgoFn ?Y)))))


(instance Ma BinaryPredicate)
(domain Ma+- 1 GeochronologicBase)
(domain Ma+- 2 Number)

(=> (Ma ?B ?X)
    (equal ?B (MillionYearsAgoFn ?X)))


