 (instance ?RETURN TaxReturn)
 (hasPurpose ?RETURN 
    (exists (?CALCULATE ?AMOUNT ?TAX)
      (and
        (instance ?CALCULATE Calculating)
        (instrument ?CALCULATE ?RETURN)
        (result ?CALCULATE ?AMOUNT)
        (transactionAmount ?TAX ?AMOUNT)
