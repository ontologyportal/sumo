(subclass DutyTax Tax)
(documentation DutyTax EnglishLanguage "A &%Tax that is levied on imports and/or exports.")
(=>
  (and
    (instance ?X DutyTax)
    (refers ?X ?OBJ)
    (patient ?X ?OBJ))
  (exists (?T ?N1 ?N2)
    (and
      (instance ?T Transfer)
      (patient ?T ?OBJ)
      (origin ?T ?N1)
      (destination ?T ?N2)
      (instance ?N1 Nation)
      (instance ?N2 Nation)
      (not (equal ?N1 ?N2))
      (earlier (WhenFn ?T) (WhenFn ?X))
      (causes ?T ?X))))
      
