(subclass ClosedWorldPredicate Predicate)

(=>
  (and
    (instance ?R ClosedWorldPredicate)
    (?R ?X ?Y))
  (or
    (and
      (equal ?A ?X)
      (equal ?B ?Y)
      (?R ?A ?B))
    (not
      (?R ?A ?B))))
