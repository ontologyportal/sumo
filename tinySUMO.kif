(disjoint Abstract Physical)
(disjoint Object Process)

(=>
  (disjoint ?CLASS1 ?CLASS2)
  (forall (?INST)
    (not
      (and
        (instance ?INST ?CLASS1)
        (instance ?INST ?CLASS2)))))
       
(=>
  (and
    (disjoint ?A ?B)
    (subclass ?C ?A))
  (disjoint ?C ?B))
  
(=>
  (instance ?R TransitiveRelation)
  (=>
    (and
      (?R ?A ?B)
      (?R ?B ?C))
    (?R ?A ?C)))

(=>
  (and
    (instance ?REL SymmetricRelation)
    (?REL ?INST1 ?INST2))
  (?REL ?INST2 ?INST1))

