(<=>
  (and
    (instance ?PM ParticulateMatter)
    (part ?P ?PM)
    (approximateDiameter ?P
      (MeasureFn ?S Micrometer))
    (greaterThan 10.0 ?S)
    (greaterThan ?S 2.5))
  (exists (?PM10)
    (and
      (instance ?PM10 CoarseParticulateMatter)
      (part ?PM10 ?PM))))

