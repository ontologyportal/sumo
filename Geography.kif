(termFormat EnglishLanguage Insoluble "insoluble")

(termFormat ChineseLanguage Insoluble "不可溶的")

(subclass Insoluble SolubilityAttribute)

(=>
  (and
    (instance ?X Substance)
    (attribute ?X Insoluble))
  (not
    (modalAttribute
      (exists (?Y ?Z)
        (and
          (instance ?Y Substance)
          (instance ?Z Solution)
          (piece ?X ?Z)
          (piece ?Y ?Z)))
       Possibility)))

