    (instance ?B PointInSpace)
    (located ?A AndesMountains)
    (located ?B AndesMountains)
    (orientation ?A ?B East)    
    (not
      (exists (?A1 ?B1)
        (and
          (instance ?A1 PointInSpace)
          (located ?A1 AndesMountains)
