    (and
      (instance ?shore ShoreArea)
      (part ?shore ?plain))))

(subclass RiverBank ShoreArea)
(subclass RiverBank LandForm)

(documentation RiverBank EnglishLanguage "A &%RiverBank is the &%ShoreArea adjacent to a &%River.")

(=>
  (instance ?bank RiverBank)
