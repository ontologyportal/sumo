      (instance ?SEARCHENGINE SearchEngine))))

(=>
  (and    
    (instance ?QUERY SearchQuery)
    (instance ?SEARCHATTEMPT SearchAttempt)
    (represents ?QUERY ?SEARCHATTEMPT))
  (exists (?AGENT ?OBJ)
    (and
      (instance ?AGENT Agent)
      (patient ?SEARCHATTEMPT ?OBJ)
      (agent ?SEARCHATTEMPT ?AGENT))))


