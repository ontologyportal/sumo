  (instance ?BRD BusinessRequirementDocument)
  (exists (?PLAN ?BU ?PROC)
    (and
      (instance ?PLAN Plan)
      (instance ?BU Business)
      (instance ?PROC Process)
      (represents ?PLAN ?PROC)
      (desires ?BU ?PROC)
      (containsInformation ?BRD ?PLAN))))


