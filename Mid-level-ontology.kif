      (instance ?V Vehicle)
      (patient ?H ?V))))

(=>
  (instance ?H Hijacking)
  (exists (?D)
    (and
      (subProcess ?D ?H)
      (instance ?D Driving))))

(=>
  (instance ?X Hijacking)
  (exists (?V ?A)
    (and
      (agent ?X ?A)
      (instance ?V TransportationDevice)
      (holdsDuring
        (ImmediatePastFn
          (WhenFn ?X))
