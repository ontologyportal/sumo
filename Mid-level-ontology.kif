  (instance ?P Pleading)
  (exists (?J)
    (and
      (instance ?J JudicialProcess)
      (subProcess ?P ?J))))

(=>
  (instance ?P Pleading)
  (exists (?C)
    (and
      (instance ?C CriminalAction)
      (refers ?P ?C))))
