      (or 
        (part ?D ?O)
        (connected ?D ?O)))))

(=>
  (equipmentCount ?O ?T ?C)
  (equipmentType ?O ?T))

(=>
  (equipmentType ?O ?T)
  (exists (?C)
    (and
      (greaterThanOrEqualTo ?C 1)
      (equipmentCount ?O ?T ?C))))

