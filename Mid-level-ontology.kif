  (exists (?LEG1 ?LEG2 ?LEG3)
    (and
      (part ?LEG1 ?X)
      (part ?LEG2 ?X)
      (part ?LEG3 ?X)
      (meetsSpatially ?LEG1 ?LEG2)
      (meetsSpatially ?LEG2 ?LEG3)
      (meetsSpatially ?LEG1 ?LEG3))))
   
(=>
  (instance ?X Tripod)
  (hasPurpose ?X
    (exists (?OBJ ?PUT ?PROC)
      (and
        (instance ?OBJ Artifact)
        (instance ?PUT Putting)
        (patient ?PUT ?OBJ)
        (destination ?PUT ?X)
        (instance ?PROC Process)
        (instrument ?PROC ?OBJ)
        (before
          (EndFn (WhenFn ?PUT))
          (BeginFn (WhenFn ?PROC)))))))
          
