(=>
  (holdsDuring ?TIME
    (and
      (instance ?YOUTH HumanYouth)
      (age ?YOUTH (MeasureFn ?NUMBER YearDuration))))
  (lessThan ?NUMBER 18))

;; NS. An alternate formulation.
(=>
  (holdsDuring ?TIME (instance ?H HumanYouth))
  (exists (?AGE)
