(=>
  (instance ?N AntiInflammatory)
  (hasPurpose ?N
    (exists (?I ?H ?IN)
      (and
        (instance ?I Inflammation)
        (experiencer ?H ?I)
        (instance ?IN Ingesting)
        (before
          (BeginFn
            (WhenFn ?I))
          (BeginFn
            (WhenFn ?IN)))
        (holdsDuring
          (FutureFn
            (WhenFn ?I))
          (not
            (exists (?I2)
              (and
                (instance ?I2 Inflammation)
                (experiencer ?I2 ?H)))))))))

