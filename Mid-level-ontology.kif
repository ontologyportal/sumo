      (ProbabilityFn
        (exists (?Y)
          (and
            (instance ?Y ?WHOLE)
            (part ?X ?Y)))))
    (equal ?NOTPARTPROB
      (ProbabilityFn
        (not
          (exists (?Z)
            (and
              (instance ?Z ?WHOLE)
              (part ?X ?Z)))))))
  (greaterThan ?PARTPROB ?NOTPARTPROB))

