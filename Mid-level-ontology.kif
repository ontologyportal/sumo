(externalImage Lever "http://upload.wikimedia.org/wikipedia/commons/thumb/f/f2/Palanca-ejemplo.jpg/800px-Palanca-ejemplo.jpg")

(=>
  (instance ?L Lever)
  (exists (?A)
    (axis ?A ?L)))

(=>
  (and
    (instance ?L Lever)
    (instance ?P Pushing)
    (instance ?M1 Motion)
    (destination ?P ?E1)
    (physicalEnd ?E1 ?L)
    (physicalEnd ?E2 ?L)
    (not
      (equal ?E1 ?E2))
    (holdsDuring (ImmediatePastFn (WhenFn ?M1))
      (located ?E1 ?P1))
    (holdsDuring (ImmediateFutureFn (WhenFn ?M1))
      (located ?E1 ?P2))
    (holdsDuring (ImmediatePastFn (WhenFn ?M1))
      (located ?E2 ?P3))
    (holdsDuring (ImmediateFutureFn (WhenFn ?M1))
      (located ?E2 ?P4))
    (distance ?P1 ?P2 ?D1)
    (distance ?P3 ?P4 ?D2))
  (greaterThan ?D2 ?D1))

(=>
  (instance ?P GunPowder)
  (capability Explosion instrument ?P))

