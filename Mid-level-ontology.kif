  (exists (?OBJ1 ?OBJ2)
      (and
        (instance ?OBJ1 Object)
        (instance ?OBJ2 VisualContentBearingObject)
        (patient ?COPY ?OBJ1)
        (result ?COPY ?OBJ2)
        (represents ?OBJ2 ?OBJ1))))
        

