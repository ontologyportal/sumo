(instance workLocation BinaryPredicate)
(domain workLocation 1 Organization)
(domain workLocation 2 StationaryArtifact)
(documentation workLocation EnglishLanguage "A &%Relation that specifies
the typical or primary location where members of an &%Organization work.")
(format EnglishLanguage workLocation "members of %1 &%work at %2")

(=>
  (and
    (workLocation ?O ?S)
    (member ?P ?O)
    (instance ?W Working)
    (agent ?W ?P))
  (modalAttribute
    (located ?W ?S)
    Likely))

