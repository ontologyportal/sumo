    (material Fabric ?X)
    (exists (?POLE1 ?POLE2 ?GROUND)
      (and
        (orientation ?POLE1 ?POLE2 Near)
        (connected ?POLE1 ?X)
        (connected ?POLE2 ?X)
        (instance ?GROUND LandArea)
        (located ?X ?GROUND)
        (orientation ?X ?GROUND Above)))))

(=>
  (instance ?X Hammock)
  (hasPurpose ?X
    (exists (?PROC)
      (and
        (instance ?PROC LyingDown)
        (eventLocated ?PROC ?X)))))
