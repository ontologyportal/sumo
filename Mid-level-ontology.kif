(instance typicalAction CaseRole)
(domainSubclass typicalAction 1 Process)
(domainSubclass typicalAction 2 Agent)
(relatedInternalConcept typicalAction agent)
(relatedInternalConcept typicalAction typicallyContainsTemporalPart)

(documentation typicalAction EnglishLanguage "(&%typicalAction ?PROCESS ?CLASS) means that instances of the class ?AGENT are typically an active determinant of instances of the &%Process class ?PROCESS.")

(=>
    (typicalAction ?PROCESS ?AGENT)
    (exists (?P ?A)
        (and
            (instance ?P ?PROCESS)
            (instance ?A ?AGENT)
            (agent ?P ?A))))
            
(=>
    (and
        (typicalAction ?PROCESS ?AGENT)
        (instance ?A ?AGENT)
        (equal ?PPROB
            (ProbabilityFn
                (exists (?X)
                    (and
                        (instance ?X ?PROCESS)
                        (agent ?X ?A)))))
        (equal ?NOTPPROB
            (ProbabilityFn
                (not
                    (exists (?Y)
                        (and
                            (instance ?Y ?PROCESS)
                            (agent ?Y ?A)))))))
    (greaterThan ?PPROB ?NOTPPROB))

      
