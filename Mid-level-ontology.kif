(subclass WorkLeave IntentionalProcess)
(documentation WorkLeave EnglishLanguage "A temporary leave from &%Working.
Note that this is disjoint from being &%Unemployed.")

(=>
   (and
      (instance ?W WorkLeave)
      (instance ?WORK Working)
      (agent ?W ?PERSON)
      (agent ?WORK ?PERSON)
      (instance ?PERSON Human))
   (not
     (overlapsTemporally
       (WhenFn ?W)
       (WhenFn ?WORK))))

(=>
  (and
    (instance ?W WorkLeave)
    (agent ?W ?PERSON))
  (holdsDuring
    (WhenFn ?W)
    (not
      (attribute ?PERSON Unemployed))))

