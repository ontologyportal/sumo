(documentation Colloid EnglishLanguage "A &%Colloid is a &%Mixture in which
microscopically dispersed &%Insoluble particles, between 0.95 and 977 &%Nanometers in
&%approximateDiameter on the Wentworth Scale are suspended throughout another
&%Substance.")
(documentation Colloid ChineseLanguage "&%Colloid  &%Mixture
 &%approximateDiameter  0.95 and 977 &%nanometer &%Insoluble
&%substance  &%substance ")

(subclass Colloid Mixture)

(=>
  (instance ?Z Colloid)
  (exists (?P ?S)
    (and
      (part ?P ?Z)
      (attribute ?P Insoluble)
      (approximateDiameter ?P
        (MeasureFn ?S Nanometer))
      (greaterThan 977.0 ?S)
      (greaterThan ?S 0.95))))

(documentation Emulsion EnglishLanguage "An &%Emulsion is a &%Mixture of two or more &%Substances in &%Liquid state that
are normally &%Immiscible owing to liquid-liquid phase separation.")
(subclass Emulsion Colloid)
(subclass Emulsion LiquidMixture)

(=>
  (instance ?X Emulsion)
  (exists (?L1 ?L2)
    (and
      (instance ?L1 Substance)
      (instance ?L2 Substance)
      (attribute ?L1 Liquid)
      (attribute ?L2 Liquid)
      (not
        (equal ?L1 ?L2)))))

(=>
  (and
    (instance ?L LiquidMixture)
    (attribute ?L Immiscible)
    (equal ?L ?X))
  (instance ?X Emulsion))

(documentation Miscible EnglishLanguage "&%Miscible describes a state when two &%Substances can mix in all
proportions, forming a homogeneous &Mixture often in the form of &%LiquidMixture.")
(instance Miscible InternalAttribute)

(=>
  (and
    (instance ?L LiquidMixture)
    (attribute ?L Miscible)
    (equal ?L ?X))
  (instance ?X Solution))

(documentation Immiscible EnglishLanguage "Contrary to &%Miscible, &%Immiscible is the state when there are
certain proportions in which the &%Mixture does not form a &%Solution.")
(instance Immiscible InternalAttribute)
(contraryAttribute Miscible Immiscible)

(documentation Latex EnglishLanguage "&%Latex is an &%Emulsion of polymer &%ParticulateMatter in water.")
(subclass Latex Emulsion)

(documentation Gel EnglishLanguage "A &%Gel is a &%Colloid which has a property lies between a &%Solid and a &%Liquid. &%Cheese
and &%AloeveraGel are examples.")
(subclass Gel Colloid)

(=>
  (and
    (instance ?X Colloid)
    (attribute ?X Solid)
    (attribute ?X Fluid)
    (equal ?X ?Y))
  (instance ?Y Gel))

(documentation SolMixture EnglishLanguage "A &%SolMixture is a colloidal &%Suspension made out of tiny &%Solid particules in a
continuous &%Liquid medium. Examples of Sol includes &%Blood &%Paint.")
(subclass SolMixture Colloid)
(subclass SolMixture LiquidMixture)
(subclass Blood SolMixture)

