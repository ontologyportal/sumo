      (uniqueIdWithRespectTo ?CORPORATION ?HUMAN ?NUMBER))))

(documentation InvoiceNumber EnglishLanguage "A &%SymbolicString for an &%Invoice which is unique to &%Seller.")
(subclass InvoiceNumber SymbolicString)

(=>
