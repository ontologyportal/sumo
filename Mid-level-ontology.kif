(=>
  (instance ?BU BuildingUnit)
  (exists (?O ?B)
    (and
      (instance ?B Building)
      (instance ?O Organization)
      (part ?BU ?B)
      (workLocation ?O ?BU))))

(subclass IntensiveCareUnit BuildingUnit)

(documentation IntensiveCareUnit EnglishLanguage "A part of a &%Hospital
where the sickest patients are cared for.")
(termFormat EnglishLanguage IntensiveCareUnit "intensive care unit")
(termFormat EnglishLanguage IntensiveCareUnit "ICU")

(=>
  (holdsDuring ?T
    (and
      (instance ?ICU IntensiveCareUnit)
      (stays ?H ?ICU)))
  (modalAttribute
    (exists (?D)
      (and
        (instance ?D Death)
        (finishes (WhenFn ?D) ?T)))
    Possibility))

