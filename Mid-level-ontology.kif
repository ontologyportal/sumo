(=>
  (and
    (instance ?UNIT SecurityUnit)
    (subOrganization ?UNIT ?ORG))
  (holdsObligation 
    (exists (?MAINTAIN)
      (and
        (instance ?MAINTAIN Maintaining)
        (agent ?MAINTAIN ?UNIT)
        (patient ?MAINTAIN ?ORG)))
