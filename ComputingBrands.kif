(termFormat JapaneseLanguage carrierFrequency "搬送波周波数")

;; lambda = c / f
;; c = 299,792,458 m/s

(=>
  (and
    (instance ?R RadiatingElectromagnetic)
    (carrierFrequency ?R (MeasureFn Hertz ?HZ))
    (wavelength ?R (MeasureFn Meter ?L)))
  (equal
    (MeasureFn Meter ?L)
    (MeasureFn Meter (DivisionFn 299792458 ?HZ))))

(=>
  (and
    (instance ?WN (NetworkCommunicationFn BluetoothNetwork))
    (instance ?R RadiatingElectromagnetic)
    (subProcess ?R ?WN)
    (carrierFrequency ?R ?W))
  (and
    (greaterThan ?W (GigaFn (MeasureFn Hertz 2.4)))
    (lessThan ?W (GigaFn (MeasureFn Hertz 2.485)))))

