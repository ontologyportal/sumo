(instance dateDissolved BinaryPredicate)
(domain dateDissolved 1 Physical)
(domainSubclass dateDissolved 2 TimePosition)

(=>
  (dateDissolved ?THING ?DATE)
  (exists (?DAY)
    (and
      (instance ?DAY ?DATE)
      (overlapsTemporally ?DAY (EndFn (WhenFn ?THING))))))

