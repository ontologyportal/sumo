    (and
      (equal ?SPEEDLIST
        (Mean3SecondWindSpeedListFn ?PLACE ?TIME))  
      (equal ?NLIST
        (PhysicalQuantityToNumberFn ?SPEEDLIST))        
      (equal ?SPEED 
        (MeasureFn (MaxValueFn ?NLIST) ?UNIT)))))
        
