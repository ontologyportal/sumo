  (exists (?TS ?DM ?A)
    (and
      (instance ?TS Thunderstorm)
      (instance ?DM MotionDownward)
      (instance ?A Air)
      (patient ?DM ?A)
      (subProcess ?DM ?TS)
      (causes ?DM ?HABOOB))))

