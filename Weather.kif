(instance BadAirQuality RelationalAttribute)

(=>
  (and
    (instance ?HAZE AtmosphericHazing)
    (instance ?PLACE GeographicArea)
    (eventLocated ?HAZE ?PLACE))
  (holdsDuring
    (WhenFn ?HAZE)
    (modalAttribute
