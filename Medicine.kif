  (and
    (instance ?CA COVIDAntibody)
    (holdsDuring ?T
      (and
        (not
          (attribute ?H Covid19))
        (part ?CA ?H))))
  (holdsDuring
    (ImmediateFutureFn ?T)
    (modalAttribute
      (attribute ?H Covid19)
      Unlikely)))

