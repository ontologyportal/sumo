(subclass Pandemic Epidemic)

(documentation Pandemic EnglishLanguage "An &%Epidemic that takes
place in at least two countries or continents.")
(termFormat EnglishLanguage Pandemic "pandemic")

(=>
  (instance ?E Pandemic)
  (exists (?H1 ?H2 ?N1 ?N2 ?C1 ?C2)
    (and
      (involvedInEvent ?E ?H1)
      (involvedInEvent ?E ?H2)
      (not
        (equal ?H1 ?H2))
      (instance ?N1 Nation)
      (instance ?N2 Nation)
      (not
        (equal ?N1 ?N2))
      (instance ?C1 Continent)
      (instance ?C2 Continent)
      (not
        (equal ?C1 ?C2))
      (holdsDuring
        (WhenFn ?E)
        (or
          (and
            (located ?H1 ?C1)
