    (part ?CA ?H))
  (modalAttribute
    (knows ?H
      (exists (?CA)
        (and
          (instance ?CA COVIDAntibody)
          (part ?CA ?H))))
    Likely))

(subclass COVIDAntibody Antibody)
(documentation COVIDAntibody EnglishLanguage "An &%Antibody that
the body produces to combat &%Covid19.")
(termFormat EnglishLanguage COVIDAntibody "COVID antibody")

