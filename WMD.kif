  (exists (?VACCINE ?HUMAN ?PUT)
    (and
      (instance ?VACCINE Vaccine)
      (instance ?PUT Putting)
      (destination ?PUT ?HUMAN)
      (instance ?HUMAN Human)
      (modalAttribute
        (holdsDuring 
          (FutureFn 
            (WhenFn ?PUT)) 
          (not 
            (attribute ?HUMAN ?DISEASE))) Likely))))

