;;If a heater is responsible for heating only 1 room, then default measurements are...	
(=>
  (and
    (memberType ?CLCT Room)
    (instance ?H RoomHeater)
    (instance ?CLCT Collection)
    (memberCount ?CLCT 1)
    (instance ?E Heating)
    (instrument ?E ?H)
    (patient ?E ?CLCT))
	(and
		(defaultMinimumHeight ?H (MeasureFn 5 Inch))
		(defaultMaximumHeight ?H (MeasureFn 16 Inch))
		(defaultMinimumWidth ?H (MeasureFn 5 Inch))
		(defaultMaximumWidth ?H (MeasureFn 7 Inch))
		(defaultMinimumLength ?H (MeasureFn 18 Inch))
		(defaultMaximumLength ?H (MeasureFn 26 Inch))
		(defaultMinimumMeasure ?H (MeasureFn 11 PoundMass))
		(defaultMaximumMeasure ?H (MeasureFn 30 PoundMass))))


;;If a heater is responsible for heating more than 1 room, then default measurements are...		
(=>
	(and
		(memberType ?CLCT Room)
    (instance ?H RoomHeater)
    (instance ?CLCT Collection)
    (memberCount ?CLCT ?C)
    (greaterThan ?C 1)
    (instance ?E Heating)
    (instrument ?E ?H)
    (patient ?E ?CLCT))
	(and
		(defaultMinimumHeight ?H (MeasureFn 16 Inch))
		(defaultMaximumHeight ?H (MeasureFn 40 Inch))
		(defaultMinimumWidth ?H (MeasureFn 10 Inch))
		(defaultMaximumWidth ?H (MeasureFn 26 Inch))
		(defaultMinimumLength ?H (MeasureFn 26 Inch))
		(defaultMaximumLength ?H (MeasureFn 45 Inch))
		(defaultMinimumMeasure ?H (MeasureFn 30 PoundMass))
		(defaultMaximumMeasure ?H (MeasureFn 200 PoundMass))))
			

;;If an Air Conditioner is responsible for cooling only 1 room, then default measurements are...
(=>
  (and
    (memberType ?CLCT Room)
    (instance ?A AirConditioner)
    (instance ?CLCT Collection)
    (memberCount ?CLCT 1)
    (instance ?E Cooling)
    (instrument ?E ?A)
    (patient ?E ?CLCT))
	(and
		(defaultMinimumHeight ?A (MeasureFn 18 Inch))
		(defaultMaximumHeight ?A (MeasureFn 28 Inch))
		(defaultMinimumWidth ?A (MeasureFn 12 Inch))
		(defaultMaximumWidth ?A (MeasureFn 21 Inch))
		(defaultMinimumLength ?A (MeasureFn 21 Inch))
		(defaultMaximumLength ?A (MeasureFn 36 Inch))
		(defaultMinimumMeasure ?A (MeasureFn 75 PoundMass))
		(defaultMaximumMeasure ?A (MeasureFn 250 PoundMass))))

 
 ;;If an Air Conditioner is responsible for cooling more than 1 room, then default measurements are...
(=>
  (and
    (memberType ?CLCT Room)
    (instance ?A AirConditioner)
    (instance ?CLCT Collection)
    (memberCount ?CLCT ?C)
    (greaterThan ?C 1)
    (instance ?E Cooling)
    (instrument ?E ?A)
    (patient ?E ?CLCT))
	(and
		(defaultMinimumHeight ?A (MeasureFn 28 Inch))
		(defaultMaximumHeight ?A (MeasureFn 60 Inch))
		(defaultMinimumWidth ?A (MeasureFn 21 Inch))
		(defaultMaximumWidth ?A (MeasureFn 60 Inch))
		(defaultMinimumLength ?A (MeasureFn 36 Inch))
		(defaultMaximumLength ?A (MeasureFn 60 Inch))
		(defaultMinimumMeasure ?A (MeasureFn 75 PoundMass))
		(defaultMaximumMeasure ?A (MeasureFn 500 PoundMass))))
    
