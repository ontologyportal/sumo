;;Characteristics Measured: Voltage, Volume, Height, Width, Bandwidth, Area, Mass


;;Define OunceMass for some mass measurements
(=>
  (measure ?OBJECT
    (MeasureFn ?NUMBER OunceMass))
  (measure ?OBJECT
    (MeasureFn 
      (DivisionFn ?NUMBER 16) PoundMass)))
      
(instance OunceMass UnitOfMass)
(documentation OunceMass EnglishLanguage "English mass unit equal to 1/16th of a &%PoundMass")


;;========================================================================================
;;								RELATION DEFINITIONS
;;========================================================================================

(=>
	(defaultMinimumHeight ?O ?H)
	(modalAttribute
		(exists (?INST)
			(and
				(instance ?INST ?O)
				(height ?INST ?HEIGHT)
				(lessThan ?HEIGHT ?H))) Unlikely))
        
(instance defaultMinimumHeight BinaryPredicate)
(domain defaultMinimumHeight 1 Object)
(domain defaultMinimumHeight 2 LengthMeasure)
(documentation defaultMinimumHeight EnglishLanguage "A &%BinaryPredicate that states the assumed minimum &%height of an &%Object")

(=>
	(defaultMaximumHeight ?O ?H)
	(modalAttribute
		(exists (?INST)
			(and
				(instance ?INST ?O)
				(height ?INST ?HEIGHT)
				(greaterThan ?HEIGHT ?H))) Unlikely))
		
(instance defaultMaximumHeight BinaryPredicate)
(domainSubclass defaultMaximumHeight 1 Object)
(domain defaultMaximumHeight 2 LengthMeasure)
(documentation defaultMaximumHeight EnglishLanguage "A &%BinaryPredicate that states the assumed maximum &%height of an &%Object")

(=>
	(defaultMinimumWidth ?O ?W)
	(modalAttribute
		(exists (?INST)
			(and
				(instance ?INST ?O)
				(width ?INST ?WIDTH)
				(lessThan ?WIDTH ?W))) Unlikely))
				
(instance defaultMinimumWidth BinaryPredicate)
(domainSubclass defaultMinimumWidth 1 Object)
(domain defaultMinimumWidth 2 LengthMeasure)
(documentation defaultMinimumWidth EnglishLanguage "A &%BinaryPredicate that states the assumed minimum &%width of an &%Object")				

(=>
	(defaultMaximumWidth ?O ?W)
	(modalAttribute
		(exists (?INST)
			(and
				(instance ?INST ?O)
				(width ?INST ?WIDTH)
				(greaterThan ?WIDTH ?W))) Unlikely))
				
(instance defaultMaximumWidth BinaryPredicate)
(domainSubclass defaultMaximumWidth 1 Object)
(domain defaultMaximumWidth 2 LengthMeasure)
(documentation defaultMaximumWidth EnglishLanguage "A &%BinaryPredicate that states the assumed maximum &%width of an &%Object")
				
(=>
	(defaultMinimumLengh ?O ?L)
	(modalAttribute
		(exists (?INST)
			(and
				(instance ?INST ?O)
				(length ?INST ?LENGTH)
				(lessThan ?LENGTH ?L))) Unlikely))

(instance defaultMinimumLength BinaryPredicate)
(domainSubclass defaultMinimumLength 1 Object)
(domain defaultMinimumLength 2 LengthMeasure)
(documentation defaultMinimumLength EnglishLanguage "A &%BinaryPredicate that states the assumed minimum &%length of an &%Object")				
				
(=>
	(defaultMaximumLength ?O ?L)
	(modalAttribute
		(exists (?INST)
			(and
				(instance ?INST ?O)
				(length ?INST ?LENGTH)
				(greaterThan ?LENGTH ?L))) Unlikely))
				
(instance defaultMaximumLength BinaryPredicate)
(domainSubclass defaultMaximumLength 1 Object)
(domain defaultMaximumLength 2 LengthMeasure)
(documentation defaultMaximumLength EnglishLanguage "A &%BinaryPredicate that states the assumed maximum &%length of an &%Object")	

(=> 
  (and
    (sphereRadius ?OBJECT ?RADIUS)
    (attribute ?OBJECT Sphere))
  (exists (?POINT)
    (forall (?PART)
      (=>
        (pointOfFigure ?PART ?OBJECT)
        (geometricDistance ?PART ?POINT ?RADIUS)))))
        
 (instance sphereRadius BinaryPredicate)
 (domain sphereRadius 1 Object)
 (domain sphereRadius 2 LengthMeasure)
 (documentation sphereRadius EnglishLanguage "(&%sphereRadius ?OBJECT ?LENGTH) means the radius of the spherical &%Object, ?OBJECT, is ?LENGTH.")

(=>
  (and
    (defaultMinimumSphereRadius ?O ?R)
    (attribute ?O Sphere))
  (modalAttribute
    (and
      (sphereRadius ?O ?RADIUS)
      (lessThan ?RADIUS ?R)) Unlikely))
        
(instance defaultMinimumSphereRadius BinaryPredicate)
(domain defaultMinimumSphereRadius 1 Object)
(domain defaultMinimumSphereRadius 2 LengthMeasure)
(documentation defaultMinimumSphereRadius EnglishLanguage "A &%BinaryPredicate that states the assumed minimum &%sphereRadius of a spherical &%Object")

(=>
  (and
    (defaultMaximumSphereRadius ?O ?R)
    (attribute ?O Sphere))
  (modalAttribute
    (exists (?INST)
      (and
        (instance ?INST ?O)
        (sphereRadius ?INST ?RADIUS)
        (greaterThan ?RADIUS ?R))) Unlikely))
        
(instance defaultMaximumSphereRadius BinaryPredicate)
(domain defaultMaximumSphereRadius 1 Object)
(domain defaultMaximumSphereRadius 2 LengthMeasure)
(documentation defaultMaximumSphereRadius EnglishLanguage "A &%BinaryPredicate that states the assumed maximum &%sphereRadius of a spherical &%Object")

(=>
	(defaultMinimumMeasure ?O (MeasureFn ?Q ?U))
	(modalAttribute
		(exists (?INST)
			(and
				(instance ?INST ?O)
				(measure ?INST (MeasureFn ?QUANTITY ?U))
				(lessThan ?QUANTITY ?Q))) Unlikely))
				
(instance defaultMinimumMeasure BinaryPredicate)
(domainSubclass defaultMinimumMeasure 1 Object)
(domain defaultMinimumMeasure 2 Quantity)
(documentation defaultMinimumMeasure EnglishLanguage "A &%BinaryPredicate that states the assumed minimum &%measure of an &%Object")



(=>
	(defaultMaximumMeasure ?O (MeasureFn ?Q ?U))
	(modalAttribute
		(exists (?INST ?QUANTITY)
			(and
				(instance ?INST ?O)
				(measure ?INST (MeasureFn ?QUANTITY ?U))
				(greaterThan ?QUANTITY ?Q))) Unlikely))

				
(instance defaultMaximumMeasure BinaryPredicate)
(domainSubclass defaultMaximumMeasure 1 Object)
(domain defaultMaximumMeasure 2 Quantity)
(documentation defaultMaximumMeasure EnglishLanguage "A &%BinaryPredicate that states the assumed maximum &%measure of an &%Object")


