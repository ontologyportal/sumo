(domainSubclass instrumentSubclass 2 Object)

(=>
  (instrumentSubclass ?PROC ?OBJ)
  (exists (?ITEM)
    (and
      (instance ?ITEM ?OBJ)
      (instrument ?PROC ?ITEM))))
      
