(termFormat EnglishLanguage DivorcedStatus "divorced")

(=>
  (attribute ?HUMAN DivorcedStatus)
  (exists (?DIVORCE)
    (and
      (instance ?DIVORCE Divorcing)
      (agent ?DIVORCE ?HUMAN))))
      
