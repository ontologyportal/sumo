;; file systems contain only files and folders
(=>
	(and
		(instance ?FS FileSystem)
		(member ?M ?FS))
	(instance ?M ComputerFile))

;; folders contain only files and other folders
(=>
	(and
		(instance ?F ComputerDirectory)
		(member ?M ?F))
	(instance ?M ComputerFile))

