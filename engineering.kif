(documentation ITProcess EnglishLanguage "A process performed on a computer by a human
operator.")
(subclass ITProcess IntentionalProcess)
(=>
	(instance ?PROCESS ITProcess)
	(exists (?AGENT ?PATIENT)
		(and
			(agent ?PROCESS ?AGENT)
			(patient ?PROCESS ?PATIENT)
			(instance ?AGENT ITAgent)
			(instance ?PATIENT Computer))))

